<%# CHANGE THIS %>

<table class="table table-striped table-dark">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Wine</th>
      <th scope="col">Strains</th>
      <%# <th scope="col">Handle</th> %>
    </tr>
  </thead>
  <tbody>
        <% @wines.each do |wine| %>
    <tr>
            <th scope="row"><%= wine.id %></th>
            <td><%= wine.name %></td>
            <ul>
            <td><%  wine.wines_strains.each_with_index do |wine_strain, index| %>
            
                <li>Cepa <%= "#{index + 1} : #{wine_strain.strain.name} | #{wine_strain.percentage.to_i}%" %> </li>
            
            <% end %></ul></td>
    </tr>
        <% end %>
</table>